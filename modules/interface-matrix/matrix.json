  {
    "emitter": "Data Orchestrator",
    "event": "opps.save.result",
    "payload": "{ ok, id?, errors? }",
    "listeners": [
      "Dialogs"
    ]
  },
  {
    "emitter": "Data Orchestrator",
    "event": "opps.updated",
    "payload": "{ id }",
    "listeners": [
      "Opportunities Table"
    ]
  },
  {
    "emitter": "Import/Export",
    "event": "data.import.report",
    "payload": "{ ok, errors?, warnings?, added?, updated? }",
    "listeners": [
      "Data Orchestrator",
      "Debug Console"
    ]
  },
  {
    "emitter": "Import/Export",
    "event": "data.export.done",
    "payload": "{ format, blobUrl }",
    "listeners": [
      "Top Banner",
      "Debug Console"
    ]
  }
