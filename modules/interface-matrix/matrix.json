  {
    "emitter": "Opportunities Table",
    "event": "dialogs.open.company",
    "payload": "{ companyId }",
    "listeners": [
      "Dialogs"
    ]
  },
  {
    "emitter": "Opportunities Table",
    "event": "dialogs.open.contact",
    "payload": "{ contactId }",
    "listeners": [
      "Dialogs"
    ]
  },
  {
    "emitter": "Dialogs",
    "event": "opps.validate.request",
    "payload": "{ draft }",
    "listeners": [
      "Data Orchestrator"
    ]
  },
  {
    "emitter": "Dialogs",
    "event": "opps.save.request",
    "payload": "{ draft }",
    "listeners": [
      "Data Orchestrator"
    ]
  },
  {
    "emitter": "Data Orchestrator",
    "event": "opps.validate.result",
    "payload": "{ ok, errors? }",
    "listeners": [
      "Dialogs"
    ]
